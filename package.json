{
  "name": "photo-contest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.8",
    "jotai": "^1.8.2",
    "next": "12.1.4",
    "react": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-query": "^3.39.2",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.1",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.7",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1"
  },
  "type": "module",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-vector-icons|@react-native-community/picker)/)"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "description": "## Prerequisites",
  "main": "next.config.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runtimerevolution/revent-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runtimerevolution/revent-app/issues"
  },
  "homepage": "https://github.com/runtimerevolution/revent-app#readme"
}
